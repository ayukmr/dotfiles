{
  "ver": "5.0.0",

  "settings": {
    "theme":       "proton",
    "density":     "default",
    "fontSize":    "xxs",
    "sidebarCSS":  true,
    "colorScheme": "ff",

    "navBtnCount":    false,
    "tabsLvlDots":    false,
    "searchBarMode":  "none",
    "showNewTabBtns": false,
    "pinnedTabsPosition": "top",

    "ctxMenuNative":      true,
    "ctxMenuRenderIcons": false,

    "animations":     true,
    "animationSpeed": "fast",
    "tabSwitchDelay": 0,

    "hideInact":       true,
    "hideEmptyPanels": false,
    "skipEmptyPanels": false,
    "containersSortByName": true,

    "subPanelHistory":   false,
    "subPanelBookmarks": false,
    "subPanelRecentlyClosedBar": false,

    "warnOnMultiTabClose":       "none",
    "warnOnMultiBookmarkDelete": "none"
  },

  "keybindings": {
    "_execute_sidebar_action": "MacCtrl+Shift+E",

    "switch_to_panel_0":  "MacCtrl+1",
    "switch_to_panel_1": "MacCtrl+2"
  }
}
